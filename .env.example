#!/bin/bash
# .env file for docker-compose
# Generate a secure session secret with: openssl rand -base64 32

SESSION_SECRET=change-me-in-production-min-32-characters
NODE_ENV=production

# Optional: BeamMP token (if required by your BeamMP instance)
BEAMMP_TOKEN=

# Docker host (change for Windows Docker Desktop)
# Windows: npipe:////./pipe/docker_engine
# Linux: unix:///var/run/docker.sock
DOCKER_HOST=unix:///var/run/docker.sock
